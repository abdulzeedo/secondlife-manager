{
    "name": "scherersoftware/cake-api-baselayer",
    "description": "scherersoftware/cake-api-baselayer plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "CakeApiBaselayer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CakeApiBaselayer\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
